{
  "private": true,
  "name": "omscentral-client",
  "version": "0.0.0",
  "description": "",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "main": "build/index.html",
  "scripts": {
    "build": "REACT_APP_NAME=$npm_package_name REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build:local": "env-cmd -f .env.local npm run build",
    "build:production": "env-cmd -f .env.production npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "cypress": "cypress",
    "dev": "react-scripts start",
    "firebase": "firebase",
    "format": "prettier --write '(src|cypress)/{**/*,*}.{js,jsx,ts,tsx,json,css,scss}' && prettier --write '*.{js,json}' && prettier --write --parser json '.firebaserc' && prettier --write --parser json '.prettierrc'",
    "format:package": "prettier-package-json --write ./package.json",
    "start": "serve -s build -p 3000",
    "test": "run-s test:formatting test:lint test:compile test:react",
    "test:compile": "npm run tsc -- --noEmit",
    "test:formatting": "prettier --check '(src|cypress)/{**/*,*}.{js,jsx,ts,tsx,json,css,scss}' && prettier --check '*.{js,json}' && prettier --check --parser json '.firebaserc' && prettier --check --parser json '.prettierrc'",
    "test:integration": "cypress run",
    "test:lint": "eslint src/ cypress/ --ext .ts,.tsx,.js,.jsx",
    "test:react": "react-scripts test",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@apollo/react-common": "3.1.4",
    "@apollo/react-hooks": "4.0.0",
    "@apollo/react-testing": "4.0.0",
    "@devexpress/dx-react-chart": "2.x",
    "@devexpress/dx-react-chart-material-ui": "2.x",
    "@devexpress/dx-react-core": "2.x",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@sentry/browser": "6.18.2",
    "@sentry/tracing": "6.18.2",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.40",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "17.0.13",
    "@types/react-helmet": "6.1.5",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "clsx": "1.1.1",
    "env-cmd": "10.1.0",
    "eslint": "7.x",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "firebase": "9.6.8",
    "firebase-tools": "10.2.2",
    "graphql": "15.x",
    "graphql-tag": "2.12.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prettier-package-json": "2.6.3",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-apollo": "3.1.5",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-hook-form": "6.x",
    "react-markdown": "7.x",
    "react-router-dom": "5.x",
    "react-scripts": "4.x",
    "react-visibility-sensor": "5.1.1",
    "typescript": "4.6.2",
    "webpack": "4.x"
  },
  "devDependencies": {
    "cypress": "9.5.1",
    "serve": "13.0.2"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
