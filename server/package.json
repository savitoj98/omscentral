    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "5.1.3",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "17.1.8",
    "@types/html-minifier": "4.0.2",
